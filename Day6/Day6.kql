let groupCount = 4; // or 14
let input = "";
range Position from 0 to strlen(input) step  1
| extend 
    Group = reverse(substring(reverse(input), Position, groupCount)) // Use the double reverse to look at the previous group of letters
    , Position = strlen(input ) - Position // reverse the position as well
| extend AllLetters = extract_all(@"([a-zA-Z]{1})", Group) /// use regex to split into a dynamic array of 1 characters
| mv-expand AllLetters // expand the array to rows
| summarize DistinctLetterCount = count_distinct(tostring(AllLetters)) by Position // find the distinct number of letters in a group
// | summarize count_distinct(AllLetters) by Position
| where Position >= groupCount and DistinctLetterCount == groupCount
| top 1 by Position asc
| project-keep Position